{
	"name": "@mikecbrant/appsyncjs-cli",
	"version": "1.0.3",
	"description": "CLI utilities for use in `APPSYNC_JS` runtime development",
	"type": "module",
	"main": "index.mjs",
	"files": [
		"cli.mjs",
		"index.mjs",
		"commands/*.mjs",
		"!test-files",
		"!**/*.test.mjs"
	],
	"bin": {
		"appsyncjs": "./cli.mjs"
	},
	"keywords": [
		"appsync",
		"appsync_js",
		"appsyncjs",
		"aws",
		"graphql",
		"resolver",
		"resolvers",
		"build",
		"cli"
	],
	"publishConfig": {
		"registry": "https://registry.npmjs.org"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mikecbrant/appsyncjs.git"
	},
	"bugs": {
		"url": "https://github.com/mikecbrant/appsyncjs/issues"
	},
	"homepage": "https://github.com/mikecbrant/appsyncjs#readme",
	"author": "Mike Brant",
	"license": "MIT",
	"scripts": {
		"build": "echo 'no build'",
		"clean": "rm -rf ./coverage ./node_modules ./test-dist",
		"lint": "eslint --fix . --no-error-on-unmatched-pattern",
		"lint:check": "eslint . --no-error-on-unmatched-pattern",
		"test": "pnpm lint && pnpm test:unit",
		"test:unit": "vitest run"
	},
	"dependencies": {
		"commander": "^12.1.0",
		"esbuild": "^0.25.9",
		"glob": "^10.4.5"
	},
	"devDependencies": {
		"@aws-appsync/utils": "2.0.3",
		"@vitest/coverage-v8": "^3.2.4",
		"vitest": "^3.2.4"
	},
	"peerDependencies": {
		"@aws-appsync/utils": "^2.0.3"
	}
}
